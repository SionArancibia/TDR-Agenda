
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(cuid())
  username String @unique
  fullName String
  password String
  gender Gender
  createdAt DateTime @default(now())
  role Role
}

enum Gender {
  male
  female
}

enum Role {
  admin
  professional
  patient
}